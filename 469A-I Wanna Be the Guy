#include<iostream>
#include <bits/stdc++.h>
using namespace std;
#define FAST_IO ios::sync_with_stdio(false), cin.tie(nullptr), cout.tie(nullptr);

int main(){
    FAST_IO;
    set<int>s;
    int l; cin>>l;
    int p; cin>>p;
    while(p--){
        int a; cin>>a; s.insert(a);
    }
    int q; cin>>q;
    while(q--){
        int a; cin>>a; s.insert(a);
    }
    if(s.size()==l) cout<<"I become the guy.";
    else cout<<"Oh, my keyboard!";

}
