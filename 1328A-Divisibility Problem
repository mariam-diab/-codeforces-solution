#include<iostream>
#include <bits/stdc++.h>
using namespace std;
#define FAST_IO ios::sync_with_stdio(false), cin.tie(nullptr), cout.tie(nullptr);

int main(){
    FAST_IO;
    int t; cin>>t;
    while(t--){
        int a, b, c; cin>>a>>b;
        c = a % b;
        cout << (c == 0 ? 0 : b - c) << '\n';
    }

}
